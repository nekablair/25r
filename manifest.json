{
    "manifest_version":2,
    "version":"1.0",
    "name":"25r",
    "description": "A simple Pomodoro timer as an extension",
    "action": {
        "permissions": ["tabs"],
            "default_popup": "popup.html",
            "default_icon": {
                "16": "25r-logo.png",
                "48": "25r-logo.png",
                "128": "25r-logo.png"
            }
    }
}